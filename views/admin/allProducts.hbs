<div class="page-content-wrapper ">

    <div class="container-fluid">

        <div class="row">
            <div class="col-sm-12">
                <div class="page-title-box">
                    <div class="btn-group float-right">
                        <ol class="breadcrumb hide-phone p-0 m-0">
                            <li class="breadcrumb-item"><a href="#">Annex</a></li>
                            <li class="breadcrumb-item"><a href="#">Pages</a></li>
                            <li class="breadcrumb-item active">starter</li>
                        </ol>
                    </div>
                    <h4 class="page-title">starter</h4>
                </div>
            </div>
        </div>
        <!-- end page title end breadcrumb -->
        <div class="row">
            <div class="col-lg-12 col-sm-12">
                <div class="card m-b-30">
                    <div class="card-body table-responsive">

                        <div class="">
                            <table id="userTable" class="table">
                                <thead>
                                    <tr>
                                        <th>Watch Name</th>
                                        <th>Brand</th>
                                        <th>Price</th>
                                        <th>Category</th>
                                        <th>Description</th>
                                        <th>Image</th>
                                        <th>Action</th>

                                    </tr>
                                </thead>


                                <tbody>
                                    {{#each products}}
                                    <tr>
                                        <td>{{this.WatchName}}</td>
                                        <td>{{this.Brand.[0]}}</td>
                                        <td>₹{{this.Price}}</td>
                                        <td>{{this.Category.[0]}}</td>
                                        <td>{{this.Description}}</td>
                                        <td><img style="width:50px;height:50px"
                                                src="/images/products/{{this.image.[0]}}"></td>                                                                                                                          
                                        <td>
                                            <a href="/admin/updateproducts/{{this._id}}"
                                                class="btn btn-success ml-auto m-2">Update</a>
                                            {{!-- <a href="/admin/deleteproducts/{{this._id}}" class="btn btn-danger waves-effect waves-light" id="alertify-labels">Delete</a> --}}
                                            

                                                                    
                                                                                       
                                            <a href="/admin/deleteproducts/{{this._id}}"                
                                                class="btn btn-danger m-2 ml-auto"
                                                onclick="return confirm('Are you want to delete {{this.WatchName}}?')">Delete</a>
                                        </td>


                                    </tr>
                                    {{/each}}


                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--end row-->

    </div><!-- container -->

</div> <!-- Page content Wrapper -->

</div> <!-- content -->

<footer class="footer">
    © 2018 Annex by Mannatthemes.
</footer>

</div>
<!-- End Right content here -->

</div>
<!-- END wrapper -->